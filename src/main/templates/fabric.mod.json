{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "name": "${mod_name}",
  "version": "${mod_version}",
  "description": "${mod_description}",
  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "xyz.imide.replanter.loaders.fabric.FabricEntrypoint"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.16.14",
    "fabric-api": "*",
    "resourcefulconfig": ">=${rconfig}",
    "minecraft": "${target_minecraft}",
    "fabric-language-kotlin": ">=${flk}"
  },
  "suggests": {
    "modmenu": "*"
  },
  "breaks": {
    "feather": "*",
    "labymod": "*",
    "ichor": "*"
  }
}